{
  "name": "@vinicius73/rollup-plugin-skypack-resolver",
  "description": "Generate the bundle of your project using Skypack CDN for external dependencies.",
  "keywords": [
    "rollup-plugin",
    "rollup",
    "skypack",
    "skypack-cdn",
    "pika",
    "pika-cdn",
    "cdn"
  ],
  "version": "2.0.2",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "repository": "https://github.com/vinicius73/rollup-plugin-pika-resolver",
  "author": "Vinicius Reis <luiz.vinicius73@gmail.com>",
  "license": "MIT",
  "private": false,
  "engines": {
    "node": ">=10"
  },
  "files": [
    "dist/*",
    "src/*"
  ],
  "scripts": {
    "build": "rm -rf dist/ && tsc -d",
    "lint": "eslint src/*.ts",
    "prepare": "yarn run build",
    "test": "jest --coverage",
    "ci": "yarn run lint && yarn run test --ci"
  },
  "dependencies": {
    "find-up": "^5.0.0"
  },
  "peerDependencies": {
    "rollup": "^2"
  },
  "devDependencies": {
    "@sucrase/jest-plugin": "^2.0.0",
    "@types/node": "^14.11.2",
    "@typescript-eslint/eslint-plugin": "^4.2.0",
    "@typescript-eslint/parser": "^4.2.0",
    "eslint": "^7.9.0",
    "eslint-config-standard": "^14.1.1",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^4.2.1",
    "eslint-plugin-standard": "^4.0.1",
    "jest": "^26.4.2",
    "rollup": "^2.28.2",
    "typescript": "^4.0.3"
  }
}
